{
  "name": "livecode-ts-scraping",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run ./src/index.ts",
    "start:build": "deno run -A --unstable-node-globals ./dist/index.js",
    "dev": "bun run --watch ./src/index.ts",
    "build": "bun build --target node --outdir ./dist ./src/index.ts",
    "compile": "bun build --compile --outfile cli ./src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.9"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "overrides": {
    "clone-deep": "^4.0.1"
  },
  "dependencies": {
    "axios": "1.7.7",
    "cheerio": "^1.0.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "express-async-handler": "1.2.0",
    "helmet": "8.0.0",
    "http-status-codes": "2.3.0",
    "lodash-es": "^4.17.21",
    "mongodb": "^6.12.0",
    "morgan": "1.10.0",
    "puppeteer": "23.6.1",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "uuid": "11.0.2"
  }
}
